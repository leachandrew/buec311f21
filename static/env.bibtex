@article{gaudetlasserrelong95,
 ISSN = {00206598, 14682354},
 URL = {http://www.jstor.org/stable/2527368},
 abstract = {We study optimal nonrenewable resource royalty contracts when the extracting agent has private information on costs. This is a dynamic incentive problem in which the repeated relationship between the principal and the agent is constrained by initial reserves. Commitment is limited to one period and costs are intertemporally independent. Compared with full information extraction, information asymmetry shifts production to the future when the optimal contract requires exhaustion in two periods. When exhaustion by all types in two periods is not warranted, the effect on the terminal period is ambiguous and the output of even the lowest cost firm is always distorted.},
 author = {Gérard Gaudet and Pierre Lasserre and Ngo Van Long},
 journal = {International Economic Review},
 number = {3},
 pages = {715-749},
 publisher = {[Economics Department of the University of Pennsylvania, Wiley, Institute of Social and Economic Research, Osaka University]},
